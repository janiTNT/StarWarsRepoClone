// = SW_COLORS

//SETTERS
//_Format = [255, 255, 255, 255]
//
_Black =    [  0,   0,   0, 255]
_DarkGrey = [ 64,  64,  64, 255]
_Grey =     [128, 128, 128, 255]
_LightGrey =[192, 192, 192, 255]
_White =    [255, 255, 255, 255]
//
_Red =      [255,   0,   0, 255]
_Orange =   [255, 166,   0, 255]
_Yellow =   [255, 255,   0, 255]
_Lime =     [166, 255,   0, 255]
_Green =    [  0, 255,   0, 255]
_Cyan =     [  0, 255, 255, 255]
_Blue =     [  0,   0, 255, 255]
_Magenta =  [255,   0, 255, 255]

_Purple =   [160,  33, 240, 255]
_Pink =     [255, 191, 204, 255]

//SPECIAL RARE COLORS
//_Teal = [0, 128, 128, 255]
//_Brown = [165, 42, 42, 255]
//_Gold = [255, 215, 0, 255]
//_Navy = [0, 0, 128, 255]
//_Lime = [0, 255, 0, 255]
//_Maroon = [128, 0, 0, 255]
//_Olive = [128, 128, 0, 255]

//CONSTRUCTERS
Black
{
    RGBA = [&~/_Black/0,&~/_Black/1,&~/_Black/2,&~/_Black/3]
    RGB = [&../RGBA/0,&../RGBA/1,&../RGBA/2]
    Float
    {
        Rf = (&../RGBA/0) /255
        Gf = (&../RGBA/1) /255
        Bf = (&../RGBA/2) /255
        Af = (&../RGBA/3) /255 //normally set per effect.
    }
}
