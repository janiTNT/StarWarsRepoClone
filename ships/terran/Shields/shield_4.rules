	//Stats
	Stats
	{
		PowerCapacity = (&~/MaxResources) / 1000

		RecPower = (&../StatsByCategory/0/Stats/PowerUsageContinuous) + (&PowerCapacity) * (&~/ShieldStatBase/ResourceDrainPerDamage)
		RecCrew = (&RecPower)
	}
	StatsByCategory
	[
		{
			NameKey = "StatCategories/SWBubbleShield"
			Stats
			{
				ShieldHP = (&~/MaxResources) / (&~/ShieldStatBase/ResourceDrainPerDamage)
				ShieldPenResist = &~/ShieldStatBase/PenetrationResistance/0
				ShieldArc = deg(360d)
				ShieldDist = (&~/ShieldStatBase/Radius)
				PowerUsageContinuous = (&~/Part/Components/PowerDrain/FromQuantity) / (&~/Part/Components/PowerDrain/Interval) / 1000
			}
		}
	]
}
